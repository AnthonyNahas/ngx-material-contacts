{
  "name": "ngx-material-contacts",
  "description": "Angular Library to manage contacts and users with a material deisgn",
  "version": "0.0.1",
  "homepage": "https://github.com/anthonynahas/ngx-material-contacts",
  "author": {
    "name": "anthonynahas",
    "url": "https://github.com/anthonynahas"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/anthonynahas/ngx-material-contacts.git"
  },
  "license": "MIT",
  "keywords": [
    "angular",
    " library",
    " ngx",
    " material",
    " material design",
    " contacts",
    " users",
    " management"
  ],
  "bugs": {
    "url": "https://github.com/anthonynahas/ngx-material-contacts/issues"
  },
  "private": true,
  "scripts": {
    "build": "gulp build",
    "test": " gulp test",
    "demo": " gulp build:demo",
    "check-travis": " travis-status --repo=anthonynahas/ngx-material-contacts --branch=master --fail-pending || echo 'Not yet passing'"
  },
  "dependencies": {
    "@angular/animations": "^5.2.10",
    "@angular/cdk": "^5.2.5",
    "@angular/common": "^5.2.10",
    "@angular/core": "^5.2.10",
    "@angular/flex-layout": "^5.0.0-beta.14",
    "@angular/forms": "^5.2.10",
    "@angular/material": "^5.2.5"
  },
  "devDependencies": {
    "@angular/animations": "5.2.10",
    "@angular/compiler": "5.2.10",
    "@angular/compiler-cli": "5.2.10",
    "@angular/platform-browser": "5.2.10",
    "@angular/platform-browser-dynamic": "5.2.10",
    "@angular/platform-server": "5.2.10",
    "@compodoc/gulp-compodoc": "0.0.8",
    "@types/jest": "21.1.9",
    "@types/lodash": "4.14.92",
    "@types/node": "8.0.44",
    "angular-cli-ghpages": "0.5.1",
    "angular2-template-loader": "0.6.2",
    "autoprefixer": "7.1.5",
    "awesome-typescript-loader": "3.3.0",
    "codelyzer": "4.0.0",
    "commitplease": "3.1.0",
    "conventional-github-releaser": "1.1.13",
    "core-js": "2.5.1",
    "css-loader": "0.28.7",
    "cssnano": "3.10.0",
    "del": "3.0.0",
    "gulp": "3.9.1",
    "gulp-bump": "2.8.0",
    "gulp-conventional-changelog": "1.1.6",
    "gulp-coveralls": "0.1.4",
    "gulp-file": "0.3.0",
    "gulp-git": "2.4.2",
    "gulp-hub": "0.8.0",
    "gulp-inline-ng2-template": "4.0.0",
    "gulp-tslint": "8.1.1",
    "gulp-util": "3.0.8",
    "html-loader": "0.5.1",
    "is-docker": "1.1.0",
    "jest": "22.0.4",
    "jest-cli": "^22.4.3",
    "jest-preset-angular": "5.2.1",
    "lodash": "4.17.4",
    "node-sass": "4.5.3",
    "postcss": "6.0.13",
    "postcss-strip-inline-comments": "0.1.5",
    "pump": "1.0.2",
    "raw-loader": "0.5.1",
    "rollup": "0.57.0",
    "rollup-plugin-commonjs": "9.1.0",
    "rollup-plugin-node-resolve": "3.2.0",
    "rollup-plugin-sourcemaps": "0.4.2",
    "rollup-plugin-uglify": "3.0.0",
    "run-sequence": "2.2.0",
    "rxjs": "5.5.2",
    "sass-loader": "6.0.6",
    "source-map-loader": "0.2.2",
    "to-string-loader": "1.1.5",
    "travis-status": "2.0.0",
    "tslint": "5.7.0",
    "typescript": "2.4.2",
    "yargs": "10.0.3",
    "zone.js": "0.8.14"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "roots": [
      "<rootDir>/src/"
    ],
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/config",
      "/dist/"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/config/setupJest.ts"
  },
  "greenkeeper": {
    "ignore": [
      "@angular/core",
      "@angular/common",
      "@angular/compiler",
      "@angular/platform-server",
      "@angular/platform-browser",
      "@angular/platform-browser-dynamic",
      "@angular/compiler-cli",
      "zone.js",
      "rxjs",
      "tslint",
      "gulp-tslint",
      "typescript",
      "awesome-typescript-loader",
      "codelyzer",
      "@angular/animations",
      "@angular/material",
      " @angular/cdk",
      " @angular/flex-layout",
      "@types/jasmine"
    ]
  },
  "commitplease": {
    "style": "angular",
    "types": [
      "feat",
      "fix",
      "docs",
      "style",
      "refactor",
      "perf",
      "test",
      "chore",
      "revert",
      "demo"
    ],
    "scope": "\\S+.*"
  }
}
